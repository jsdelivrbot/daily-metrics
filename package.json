{
  "name": "daily-records",
  "version": "0.0.1",
  "description": "daily recording of various metrics",
  "main": "index.js",
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "build": "rm -rf dist/* && npm run copy:others && babel src --out-dir dist --presets es2015",
    "copy:others": "rsync -avm --exclude='*.js' ./src/* ./dist",
    "start": "npm run build && nodemon ./dist/index.js"
  },
  "author": "Tom Scott",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-preset-es2015": "^6.22.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "body-parser": "^1.16.1",
    "express": "^4.14.1",
    "mocha": "^3.2.0",
    "mongoose": "^4.8.3",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0"
  }
}
