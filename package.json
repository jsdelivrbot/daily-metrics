{
  "name": "daily-records",
  "version": "0.0.1",
  "description": "daily recording of various metrics",
  "engines": {
    "node": "6.9.0"
  },
  "main": "index.js",
  "scripts": {
    "dev": "npm run dev-build && nodemon ./lib/index.js",
    "test": "npm run dev-build && npm run test-build && NODE_ENV=test mocha",
    "test-build": "rm -rf test/* && babel --presets es2015,react test-src -d test",
    "dev-build": "rm -rf lib/* && babel-node --presets es2015,react src -d lib",
    "start": "npm run dev-build && node ./lib/index.js"
  },
  "author": "Tom Scott",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-preset-es2015": "^6.22.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.16.1",
    "cors": "^2.8.1",
    "express": "^4.14.1",
    "jwt-simple": "^0.5.1",
    "mocha": "^3.2.0",
    "mongoose": "^4.8.3",
    "morgan": "^1.8.1",
    "nodemon": "^1.11.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-form": "^6.5.0",
    "redux-promise": "^0.5.3",
    "supertest": "^3.0.0"
  }
}
